Subject: [PATCH 027/122] www: add translation of JFFS2

Signed-off-by: Rokis <ghost1988102@gmail.com>
---
 release/src/router/www/Advanced_System_Content.asp | 6 +++---
 release/src/router/www/CN.dict                     | 3 +++
 release/src/router/www/EN.dict                     | 3 +++
 release/src/router/www/TW.dict                     | 3 +++
 4 files changed, 12 insertions(+), 3 deletions(-)

diff --git a/release/src/router/www/Advanced_System_Content.asp b/release/src/router/www/Advanced_System_Content.asp
index cb530ef99c..ed67dbda5c 100644
--- a/release/src/router/www/Advanced_System_Content.asp
+++ b/release/src/router/www/Advanced_System_Content.asp
@@ -2021,18 +2021,18 @@ function pullNTPList(obj){
 			<table width="100%" border="1" align="center" cellpadding="4" cellspacing="0" bordercolor="#6b8fa3"  class="FormTable">
 				<thead>
 					<tr>
-						<td colspan="2">Persistent JFFS2 partition</td>
+						<td colspan="2"><#JFFS2_Persistent#></td>
 					</tr>
 				</thead>
 				<tr>
-					<th>Format JFFS partition at next boot</th>
+					<th><#JFFS2_Format_Next#></th>
 					<td>
 						<input type="radio" name="jffs2_format" value="1" onclick="warn_jffs_format();" <% nvram_match("jffs2_format", "1", "checked"); %>><#checkbox_Yes#>
 						<input type="radio" name="jffs2_format" value="0" <% nvram_match("jffs2_format", "0", "checked"); %>><#checkbox_No#>
 					</td>
 				</tr>
 				<tr>
-					<th>Enable JFFS custom scripts and configs</th>
+					<th><#JFFS2_Scripts#></th>
 					<td>
 						<input type="radio" name="jffs2_scripts" value="1" <% nvram_match("jffs2_scripts", "1", "checked"); %>><#checkbox_Yes#>
 						<input type="radio" name="jffs2_scripts" value="0" <% nvram_match("jffs2_scripts", "0", "checked"); %>><#checkbox_No#>
diff --git a/release/src/router/www/CN.dict b/release/src/router/www/CN.dict
index 4491974699..b4c23cbee0 100644
--- a/release/src/router/www/CN.dict
+++ b/release/src/router/www/CN.dict
@@ -4084,3 +4084,6 @@ weather_6h=6小时
 weather_12h=12小时
 weather_24h=24小时
 WLANConfig11b_TxPower_custom=自定义功率
+JFFS2_Persistent=JFFS2分区设置
+JFFS2_Format_Next=下次启动时格式化JFFS分区
+JFFS2_Scripts=启用JFFS自定义脚本和配置
diff --git a/release/src/router/www/EN.dict b/release/src/router/www/EN.dict
index ecc8c7f435..b128270fde 100644
--- a/release/src/router/www/EN.dict
+++ b/release/src/router/www/EN.dict
@@ -4084,3 +4084,6 @@ weather_6h=6 hours
 weather_12h=12 hours
 weather_24h=24 hours
 WLANConfig11b_TxPower_custom=Customize Tx Power
+JFFS2_Persistent=Persistent JFFS2 partition
+JFFS2_Format_Next=Format JFFS partition at next boot
+JFFS2_Scripts=Enable JFFS custom scripts and configs
diff --git a/release/src/router/www/TW.dict b/release/src/router/www/TW.dict
index 1b89d7153f..e07fdd901e 100644
--- a/release/src/router/www/TW.dict
+++ b/release/src/router/www/TW.dict
@@ -4084,3 +4084,6 @@ weather_6h=6小時
 weather_12h=12小時
 weather_24h=24小時
 WLANConfig11b_TxPower_custom=自定義功率
+JFFS2_Persistent=JFFS2分區設置
+JFFS2_Format_Next=下次啟動時格式化JFFS分區
+JFFS2_Scripts=啟用JFFS自定義腳本和配置
